<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd">

	<cache name="generics" eternal="true" maxElementsInMemory="0"
		overflowToDisk="false" timeToLiveSeconds="14400"
		memoryStoreEvictionPolicy="LRU">
		<persistence strategy="none" />
	</cache>

	<cache name="restApiAuthTokenCache" eternal="false"
		maxElementsInMemory="0" overflowToDisk="false" timeToLiveSeconds="14400"
		memoryStoreEvictionPolicy="LRU">
		<persistence strategy="none" />
	</cache>

</ehcache>